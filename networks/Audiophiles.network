<?xml version="1.0" encoding="UTF-8"?>
<Network xmlns="http://soap.sforce.com/2006/04/metadata">
    <allowMembersToFlag>true</allowMembersToFlag>
    <allowedExtensions>jpg,docx,txt,png,gif,bmp,pdf,xlsx</allowedExtensions>
    <branding>
        <loginFooterText>Cirrus Audiophiles</loginFooterText>
        <loginLogo>Communities_Shared_Document_Folder/cirruslogo_png.png</loginLogo>
        <primaryColor>#1797C0</primaryColor>
        <primaryComplementColor>#FFFFFF</primaryComplementColor>
        <quaternaryColor>#B1BAC1</quaternaryColor>
        <quaternaryComplementColor>#222222</quaternaryComplementColor>
        <secondaryColor>#51606E</secondaryColor>
        <tertiaryColor>#DDE4E9</tertiaryColor>
        <tertiaryComplementColor>#222222</tertiaryComplementColor>
        <zeronaryColor>#51606E</zeronaryColor>
        <zeronaryComplementColor>#FFFFFF</zeronaryComplementColor>
    </branding>
    <changePasswordTemplate>unfiled$public/CommunityChangePasswordEmailTemplate</changePasswordTemplate>
    <description>This is a B2C community using the Napili template and accessible by Larry Baxter.</description>
    <emailSenderAddress>sdo@salesforce.com</emailSenderAddress>
    <emailSenderName>Audiophiles Community</emailSenderName>
    <enableGuestChatter>true</enableGuestChatter>
    <enableInvitation>false</enableInvitation>
    <enableKnowledgeable>true</enableKnowledgeable>
    <enableNicknameDisplay>true</enableNicknameDisplay>
    <enablePrivateMessages>true</enablePrivateMessages>
    <enableReputation>true</enableReputation>
    <forgotPasswordTemplate>unfiled$public/CommunityForgotPasswordEmailTemplate</forgotPasswordTemplate>
    <maxFileSizeKb>20480</maxFileSizeKb>
    <navigationLinkSet>
        <navigationMenuItem>
            <label>Topics</label>
            <position>1</position>
            <publiclyAvailable>true</publiclyAvailable>
            <target>Topics</target>
            <type>NavigationalTopic</type>
        </navigationMenuItem>
        <navigationMenuItem>
            <defaultListViewId>All_ChatterGroups</defaultListViewId>
            <label>Community Groups</label>
            <position>2</position>
            <publiclyAvailable>false</publiclyAvailable>
            <target>CollaborationGroup</target>
            <type>SalesforceObject</type>
        </navigationMenuItem>
        <navigationMenuItem>
            <label>Support</label>
            <position>3</position>
            <publiclyAvailable>false</publiclyAvailable>
            <target>/support</target>
            <type>InternalLink</type>
        </navigationMenuItem>
        <navigationMenuItem>
            <label>My Files</label>
            <position>4</position>
            <publiclyAvailable>false</publiclyAvailable>
            <target>/my-files</target>
            <type>InternalLink</type>
        </navigationMenuItem>
        <navigationMenuItem>
            <label>Ideas</label>
            <position>5</position>
            <publiclyAvailable>true</publiclyAvailable>
            <target>/ideas</target>
            <type>InternalLink</type>
        </navigationMenuItem>
        <navigationMenuItem>
            <label>Training</label>
            <position>6</position>
            <publiclyAvailable>true</publiclyAvailable>
            <target>https://developer.salesforce.com/trailhead</target>
            <targetPreference>OpenExternalLinkInSameTab</targetPreference>
            <type>ExternalLink</type>
        </navigationMenuItem>
    </navigationLinkSet>
    <networkMemberGroups>
        <permissionSet>Moderate_Audiophiles_Community</permissionSet>
        <profile>*service</profile>
        <profile>community member - login-based</profile>
        <profile>admin</profile>
        <profile>community member - named</profile>
    </networkMemberGroups>
    <picassoSite>Audiophiles_C</picassoSite>
    <reputationLevels>
        <level>
            <label>Listener</label>
            <lowerThreshold>0.0</lowerThreshold>
        </level>
        <level>
            <label>Speaker</label>
            <lowerThreshold>26.0</lowerThreshold>
        </level>
        <level>
            <label>Preamp</label>
            <lowerThreshold>51.0</lowerThreshold>
        </level>
        <level>
            <label>Amplifier</label>
            <lowerThreshold>76.0</lowerThreshold>
        </level>
        <level>
            <label>Audio Pro</label>
            <lowerThreshold>101.0</lowerThreshold>
        </level>
        <level>
            <label>Hifi Master</label>
            <lowerThreshold>151.0</lowerThreshold>
        </level>
    </reputationLevels>
